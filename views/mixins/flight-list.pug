mixin flights(flights)
    ul.flights__list
        - for (let flight of flights)
            //pre=h.dump(flight)
            li.flights__item
                .flights__go-to 
                    a(href=`/search/flight-card/${flight.slug}`) >
                span.flights__head
                    .flights__add-date=`Добавлено ${ h.moment(flight.created_at).locale('ru').fromNow() }`
                    .flights__type
                        span.flights__type__text Тип автомобиля
                        span.flights__type__car=`${flight.author.car ? flight.author.car.type : 'Грузовой'}`
                .flights__date.flights__date__mobile
                    span=`${ h.moment(flight.date, "MMMM D, YYYY H:mm", "ru", true ).format("DD.MM") }`
                    div.flights__author #{flight.author.name}
                        div водитель
                span.flights__body
                    span.flights__item__left
                        .flights__number=`Рейс ${flight.flight_number}`
                        .flights__date 
                            span=`${ h.moment(flight.date, "MMMM D, YYYY H:mm", "ru", true ).format("DD.MM") }`
                            span Отбытие
                    span.flights__item__center
                        .flights__from-to
                            .flights__from #{flight.from}
                            svg(class="from-to-arrow" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="677" height="74" viewBox="0 0 677 74")
                                image(id="Слой_0" data-name="Слой 0" width="677" height="74" xlink:href="data:img/png;base64,")
                            .flights__to #{flight.to}
                        .flights__info
                            span Вес и объем: 
                            span 1500 кг, 150 м3
                    span.flights__item__right
                        a(href=`/search/flight-card/${flight.slug}`).button.flights__button Перейти к рейсу
                
        